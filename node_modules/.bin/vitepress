#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/gh-code/vast-video-tutorial/node_modules/.pnpm/registry.nlark.com+vitepress@0.14.1/node_modules/vitepress/bin/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules/.pnpm/registry.nlark.com+vitepress@0.14.1/node_modules/vitepress/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules/.pnpm/registry.nlark.com+vitepress@0.14.1/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules/.pnpm/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules:/mnt/d/gh-code/node_modules:/mnt/d/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules/vitepress/bin/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules/vitepress/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/gh-code/vast-video-tutorial/node_modules/.pnpm/registry.nlark.com+vitepress@0.14.1/node_modules/vitepress/bin/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules/.pnpm/registry.nlark.com+vitepress@0.14.1/node_modules/vitepress/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules/.pnpm/registry.nlark.com+vitepress@0.14.1/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules/.pnpm/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules:/mnt/d/gh-code/node_modules:/mnt/d/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules/vitepress/bin/node_modules:/mnt/d/gh-code/vast-video-tutorial/node_modules/vitepress/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
